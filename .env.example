# Example .env file for Document Information Extractor

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
# Example: OPENAI_API_KEY=sk-XXXXXXXXXXXXXXXXXXXX

# Deepseek API Configuration
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com
# Example: DEEPSEEK_API_KEY=sk-XXXXXXXXXXXXXXXXXXXX

# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434
# Example: OLLAMA_BASE_URL=http://localhost:11434

# OpenAI Compatible Providers Configuration
# Format: PROVIDER_<NAME>_API_KEY, PROVIDER_<NAME>_BASE_URL, PROVIDER_<NAME>_MODEL
# Examples:

# Azure OpenAI Configuration
PROVIDER_AZURE_NAME=Azure OpenAI
PROVIDER_AZURE_API_KEY=your_azure_api_key_here
PROVIDER_AZURE_BASE_URL=https://your-resource.openai.azure.com
PROVIDER_AZURE_MODEL=gpt-35-turbo
PROVIDER_AZURE_API_VERSION=2024-02-15-preview

# OpenRouter Configuration
PROVIDER_OPENROUTER_NAME=OpenRouter
PROVIDER_OPENROUTER_API_KEY=your_openrouter_api_key_here
PROVIDER_OPENROUTER_BASE_URL=https://api.openrouter.ai/api/v1
PROVIDER_OPENROUTER_MODEL=openai/gpt-3.5-turbo

# Mistral Configuration
PROVIDER_MISTRAL_NAME=Mistral AI
PROVIDER_MISTRAL_API_KEY=your_mistral_api_key_here
PROVIDER_MISTRAL_BASE_URL=https://api.mistral.ai/v1
PROVIDER_MISTRAL_MODEL=mistral-medium

# Together AI Configuration
PROVIDER_TOGETHER_NAME=Together AI
PROVIDER_TOGETHER_API_KEY=your_together_api_key_here
PROVIDER_TOGETHER_BASE_URL=https://api.together.xyz/v1
PROVIDER_TOGETHER_MODEL=mistral-7b-instruct

# Anyscale Configuration
PROVIDER_ANYSCALE_NAME=Anyscale
PROVIDER_ANYSCALE_API_KEY=your_anyscale_api_key_here
PROVIDER_ANYSCALE_BASE_URL=https://api.endpoints.anyscale.com/v1
PROVIDER_ANYSCALE_MODEL=mistralai/Mixtral-8x7B-Instruct-v0.1

# Add more providers using the same pattern:
# PROVIDER_<NAME>_NAME=Display Name
# PROVIDER_<NAME>_API_KEY=api_key
# PROVIDER_<NAME>_BASE_URL=base_url
# PROVIDER_<NAME>_MODEL=default_model
# PROVIDER_<NAME>_API_VERSION=api_version (optional)
